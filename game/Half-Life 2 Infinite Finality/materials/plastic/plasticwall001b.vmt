// envmaptint_fix
SDK_LightmappedGeneric
{
	"$basetexture" "plastic/plasticwall001b"
	"%keywords" "portal"

	// NOTE: Even though env_cubemap is here, it will turned off for DX6
	// because $multipass 1 isn't defined
	"$envmap" "env_cubemap"
	"$envmaptint" "[ .03 .03 .03 ]"
	"$envmapcontrast" 0.5
	"$envmapsaturation" 1

	SDK_LightmappedGeneric_HDR_DX9
	{
		"$bumpmap" "plastic/plasticwall001b_normal"
	}

	SDK_LightmappedGeneric_DX9
	{
		"$bumpmap" "plastic/plasticwall001b_normal"
	}

	SDK_LightmappedGeneric_DX8
	{
		"$bumpmap" "plastic/plasticwall001b_normal"
		"$nodiffusebumplighting" 1
	}

	SDK_LightmappedGeneric_NoBump_DX8
	{
		"$basetexture" "plastic/plasticwall001b"
	}
}
